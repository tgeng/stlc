let r1 = <rectangle {
  width 2,
  height 3
}> in
let t1 = <triangle {
  width 2,
  height 3
}> in
let area = (\s:<rectangle:{width:Double, height:Double},
               triangle:{width:Double, height:Double}>.
               match s {
                 rectangle s => s.width * s.height,
                 triangle s => s.width * s.height * 0.5
               }) in
area r1 + area t1

